{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "JFrog Fly Documentation",
  "colors": {
    "primary": "#40BE46",
    "light": "#5CDB61",
    "dark": "#1A472A"
  },
  "favicon": "/favicon.svg",
  "background": {
    "style": "gradient"
  },
  "font": {
    "headings": {
      "family": "Inter",
      "weight": 700
    },
    "body": {
      "family": "Inter",
      "weight": 400
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "introduction"
            ]
          },
          {
            "group": "Getting Started",
            "pages": [
              "getting-started/overview",
              "getting-started/account-setup",
              "getting-started/git-connection",
              "getting-started/desktop-app"
            ]
          },
          {
            "group": "Developer Workflow",
            "pages": [
              "developer-workflow/overview",
              "developer-workflow/local-artifact-management",
              "developer-workflow/automated-workflows",
              "developer-workflow/runtime-deployment"
            ]
          },
          {
            "group": "Fly Desktop App",
            "pages": [
              "desktop-app/overview",
              "desktop-app/configuration",
              "desktop-app/commands"
            ]
          },
          {
            "group": "Fly Web",
            "pages": [
              "fly-web/overview",
              "fly-web/git-repositories",
              "fly-web/artifacts",
              "fly-web/releases",
              "fly-web/workflows",
              "fly-web/users-access",
              "fly-web/fly-chat"
            ]
          }
        ]
      },
      {
        "tab": "Package Managers",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "package-managers/overview"
            ]
          },
          {
            "group": "Containers",
            "pages": [
              "package-managers/containers/docker",
              "package-managers/containers/podman"
            ]
          },
          {
            "group": "JavaScript & TypeScript",
            "pages": [
              "package-managers/javascript/npm"
            ]
          },
          {
            "group": "Python",
            "pages": [
              "package-managers/python/pip",
              "package-managers/python/twine",
              "package-managers/python/pipenv"
            ]
          },
          {
            "group": "Go",
            "pages": [
              "package-managers/go/overview"
            ]
          },
          {
            "group": "Java",
            "pages": [
              "package-managers/java/maven",
              "package-managers/java/gradle"
            ]
          },
          {
            "group": ".NET",
            "pages": [
              "package-managers/dotnet/nuget",
              "package-managers/dotnet/dotnet-cli"
            ]
          }
        ]
      },
      {
        "tab": "Resources",
        "groups": [
          {
            "group": "Help",
            "pages": [
              "resources/faq",
              "resources/troubleshooting",
              "resources/glossary"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "resources/cli-reference",
              "resources/api-reference"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Fly Web",
          "href": "https://fly.jfrog.ai",
          "icon": "rocket"
        },
        {
          "anchor": "Community",
          "href": "https://github.com/jfrog",
          "icon": "github"
        },
        {
          "anchor": "Support",
          "href": "mailto:fly-support@jfrog.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:fly-support@jfrog.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://fly.jfrog.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/jfrog",
      "github": "https://github.com/jfrog",
      "linkedin": "https://linkedin.com/company/jfrog"
    },
    "links": [
      {
        "title": "Company",
        "items": [
          {
            "label": "JFrog.com",
            "href": "https://jfrog.com"
          },
          {
            "label": "Terms of Use",
            "href": "https://jfrog.com/terms-of-use/"
          },
          {
            "label": "Privacy Notice",
            "href": "https://jfrog.com/privacy-notice/"
          }
        ]
      }
    ]
  },
  "seo": {
    "indexHiddenPages": false
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "search": {
    "prompt": "Search Fly documentation..."
  }
}
